            <solid android:color="#66FFFFFF" />
            <stroke android:color="#FFFFFFFF"
                android:width="1px" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#33FFFFFF" />
            <stroke android:color="#FFFFFFFF"
                android:width="1px" />
        </shape>
    </item>

</selector>